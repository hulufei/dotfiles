{
  "javascript.suggest.enabled": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "json",
    "svelte",
    "reason",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "args": ["--stdio"],
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "runtime": "node",
        "elmPath": "elm",
        "elmFormatPath": "elm-format",
        "elmTestPath": "elm-test"
      }
    }
  }
}
